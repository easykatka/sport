{
    "name": "sport-app",
    "version": "0.0.1",
    "description": "Веб-приложение для организации спортивных мероприятий",
    "author": "Evgeny Ovsyannikov",
    "private": true,
    "license": "MIT",
    "scripts": {
        "build": "npm run build:next && npm run build:nest",
        "build:next": "next build",
        "build:nest": "nest build --path ./tsconfig.server.json",
        "format": "prettier --write \"**/*.ts\" \"test/**/*.ts\"",
        "start": "cross-env NODE_ENV=production node dist/server/main.js",
        "start:dev": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch --path tsconfig.server.json",
        "start:prod": "cross-env NODE_ENV=production nest build --path tsconfig.server.json",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
    },
    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@hapi/joi": "^17.1.1",
        "@hookform/resolvers": "^2.8.8",
        "@mui/icons-material": "^5.6.2",
        "@mui/material": "^5.6.3",
        "@mui/styles": "^5.6.2",
        "@mui/system": "^5.6.3",
        "@nestjs/common": "^8.4.4",
        "@nestjs/config": "^2.0.0",
        "@nestjs/core": "^8.4.4",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "^8.4.4",
        "@nestjs/serve-static": "^2.2.2",
        "@nestjs/swagger": "^5.2.1",
        "@nestjs/typeorm": "^8.0.3",
        "@types/hapi__joi": "^17.1.8",
        "app-root-path": "^3.0.0",
        "axios": "^0.27.0",
        "bcryptjs": "^2.4.3",
        "cache-manager": "^3.6.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "cookie-parser": "^1.4.6",
        "cross-env": "^7.0.3",
        "date-fns": "^2.28.0",
        "fs-extra": "^10.1.0",
        "install": "^0.13.0",
        "macro-css": "^1.0.5",
        "mobx": "^6.5.0",
        "mobx-react": "^7.3.0",
        "nest-next": "^10.0.0",
        "next": "^12.1.5",
        "nookies": "^2.5.2",
        "npm": "^8.7.0",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "pg": "^8.7.3",
        "pg-hstore": "^2.3.4",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-hook-form": "^7.30.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.5",
        "sass": "^1.50.1",
        "sharp": "^0.30.4",
        "swagger-ui-express": "^4.3.0",
        "telegraf": "^4.7.0",
        "typeorm": "^0.3.6",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.2.5",
        "@nestjs/schematics": "^8.0.10",
        "@nestjs/testing": "^8.4.4",
        "@types/app-root-path": "^1.2.4",
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.13",
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "27.4.1",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.27",
        "@types/passport-jwt": "^3.0.6",
        "@types/react": "^18.0.7",
        "@types/react-dom": "^18.0.0",
        "@types/sharp": "^0.30.2",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "eslint": "^8.14.0",
        "eslint-config-next": "^12.1.5",
        "jest": "^28.0.0",
        "prettier": "^2.6.2",
        "run-script-webpack-plugin": "^0.0.11",
        "source-map-support": "^0.5.21",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.4",
        "ts-loader": "^9.2.8",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.3",
        "webpack": "^5.72.0",
        "webpack-node-externals": "^3.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
